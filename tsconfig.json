{
  "compilerOptions": {
    "baseUrl": "src",
    "inlineSources": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "lib": ["ES7", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": false,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react"
  },
  "exclude": ["node_modules"]
}
