{
  "name": "dash-material-components",
  "description": "Dash Material Design components for the Neptune platform.",
  "private": false,
  "author": "Noos Energy <contact@noos.energy>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/noosenergy/dash-material-components.git"
  },
  "version": "1.1.1",
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "scripts": {
    "format:eslint": "eslint ./src --fix",
    "format:prettier": "prettier ./src --config .prettierrc --write",
    "format": "yarn run format:prettier & yarn run format:eslint",
    "lint:eslint": "eslint ./src",
    "lint:prettier": "prettier ./src --config .prettierrc --list-different",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "build:js": "webpack --mode production",
    "build:py": "poetry run dash-generate-components ./src/components dash_material_components -p package-info.json",
    "build": "yarn run build:js && yarn run build:py",
    "postbuild": "es-check es6 dash_material_components/*.js",
    "watch": "webpack --watch --mode development",
    "test": "jest"
  },
  "dependencies": {
    "@date-io/core": "^1.0",
    "@date-io/date-fns": "^1.0",
    "@material-ui/core": "^4.0",
    "@material-ui/icons": "^4.0",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.0",
    "@material-ui/styles": "^4.0",
    "date-fns": "^2.0",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@plotly/dash-component-plugins": "^1.2.0",
    "@plotly/webpack-dash-dynamic-import": "^1.2.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "12.x",
    "babel-loader": "^8.2.2",
    "es-check": "^5.2.4",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.3.2",
    "react": "^16.0",
    "react-docgen": "^4.1.1",
    "react-dom": "^16.0",
    "styled-jsx": "^3.4.4",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.0",
    "react": "^16.0",
    "react-dom": "^16.0"
  }
}
